  "description": "",
  "main": "index.js",
  "scripts": {
    "ver": "standard-version -m 'chore(release): v%s'",
    "postver": "git push --follow-tags origin master && npm publish",
    "lint": "eslint .",
    "fix": "eslint --fix ./src",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonChen1982/bodymovin-keyframes.git"
  },
  "author": {
    "name": "jasonChen1982",
    "email": "jasonchen1882@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonChen1982/bodymovin-keyframes/issues"
  },
  "homepage": "https://github.com/jasonChen1982/bodymovin-keyframes#readme",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "release",
        "revert",
        "deps"
      ]
    }
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-egg": "^7.0.0",
    "standard-version": "^4.4.0",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "chalk": "^2.4.1",
    "inquirer": "^6.0.0",
    "stylus": "^0.54.5",
    "yargs": "^12.0.1"
  }
